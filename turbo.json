{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "signature": true
  },
  "envMode": "loose",
  "globalEnv": [
    "SENTRY_AUTH_TOKEN",
    "NEXTAUTH_SECRET",
    "NEW_RELIC_APP_NAME",
    "NEW_RELIC_LICENSE_KEY",
    "VITE_SENTRY",
    "SENTRY_DSN",
    "PORT",
    "GOOGLE_WEB_CLIENT_ID",
    "GOOGLE_WEB_CLIENT_SECRET",
    "APPLE_CLIENT_ID",
    "APPLE_CLIENT_SECRET",
    "APPLE_REDIRECT_URI",
    "VITE_API_URL",
    "STRIPE_SECRET_KEY",
    "STRIPE_PRICE_CREDIT_ID",
    "STRIPE_PRICE_CREDITS_ID",
    "STRIPE_PRICE_PLUS_ID",
    "STRIPE_PRICE_PRO_ID",
    "STRIPE_WEBHOOK_SECRET",
    "CRON_SECRET",
    "GIT_SHA",
    "ARCJET_KEY",
    "INTERNAL_API_URL",
    "API_URL",
    "ZEPTOMAIL_API_KEY",
    "FRONTEND_URL",
    "OPENAI_API_KEY",
    "CHATGPT_API_KEY",
    "CHATGPT_API_KEY_2",
    "CLAUDE_API_KEY",
    "GEMINI_API_KEY",
    "DEEPSEEK_API_KEY",
    "ANTHROPIC_API_KEY",
    "CLAUDE_API_KEY",
    "OPENROUTER_API_KEY",
    "GOOGLE_GENERATIVE_AI_API_KEY",
    "REPLICATE_API_KEY",
    "PERPLEXITY_API_KEY",
    "WEATHER_API_KEY",
    "MAXMIND_ACCOUNT_ID",
    "MAXMIND_LICENSE_KEY",
    "S3_ENDPOINT",
    "S3_ACCESS_KEY_ID",
    "S3_SECRET_ACCESS_KEY",
    "S3_REGION",
    "S3_BUCKET_NAME",
    "S3_BUCKET_NAME_APPS",
    "S3_PUBLIC_URL",
    "MALWARE_SCANNER_URL",
    "MALWARE_SCANNER_API_KEY",
    "VITE_VAPID_PUBLIC_KEY",
    "VAPID_PRIVATE_KEY",
    "VITE_CI",
    "CI",
    "VITE_NODE_ENV",
    "TEST_MEMBER_FINGERPRINTS",
    "TEST_GUEST_FINGERPRINTS",
    "TEST_MEMBER_EMAILS",
    "GOOGLE_ADS_CONVERSION_ID",
    "GOOGLE_ADS_SIGNUP_LABEL",
    "GOOGLE_ADS_PURCHASE_LABEL",
    "GOOGLE_ADS_TRIAL_LABEL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": [".next/**", "!.next/cache/**", "build/**", "dist/**"],
      "persistent": false,
      "cache": true
    },
    "build:chrome": {
      "dependsOn": ["^build"],
      "outputs": ["dist-chrome.zip"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false,
      "persistent": true,
      "env": ["DB_PROD_URL", "NODE_ENV"]
    },

    "start": {
      "cache": false,
      "persistent": true
    },
    "start:driverTrackingService": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "test:coverage": {
      "cache": false
    },
    "test:mutate": {
      "cache": false,
      "persistent": true
    },
    "test:update": {
      "cache": false,
      "persistent": true
    },
    "e2e": {
      "cache": false,
      "persistent": true
    },
    "native:dev": {
      "cache": false,
      "persistent": true
    },
    "native:build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "start:ws": {
      "cache": false,
      "persistent": true
    },
    "start:e2e": {
      "cache": false,
      "persistent": true
    },
    "start:api": {
      "cache": false,
      "persistent": true
    },
    "start:flash": {
      "cache": false,
      "persistent": true
    },
    "check-types": {
      "outputs": [],
      "cache": true,
      "persistent": false
    }
  }
}
