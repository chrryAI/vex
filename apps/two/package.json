{
  "name": "@repo/two",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "pnpm exec vite build --outDir dist/client",
    "build:server": "pnpm exec vite build --outDir dist/server --ssr src/entry-server.tsx",
    "preview": "cross-env NODE_ENV=production node server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "express": "^4.18.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^5.4.11",
    "typescript": "^5.8.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.12",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "cross-env": "^7.0.3",
    "sirv": "^2.0.4"
  }
}
