{
  "name": "@chrryai/pepper",
  "version": "2.0.44",
  "description": "Universal router for React - works in web, React Native, and browser extensions with built-in view transitions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/web.d.ts",
      "import": "./dist/web.mjs",
      "require": "./dist/web.js"
    },
    "./native": {
      "types": "./dist/native.d.ts",
      "import": "./dist/native.mjs",
      "require": "./dist/native.js"
    },
    "./extension": {
      "types": "./dist/extension.d.ts",
      "import": "./dist/extension.mjs",
      "require": "./dist/extension.js"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "pnpm exec tsup",
    "dev": "pnpm exec tsup --watch",
    "lint": "biome check .",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "router",
    "react",
    "react-native",
    "browser-extension",
    "view-transitions",
    "navigation",
    "history",
    "universal",
    "typescript"
  ],
  "author": "Iliyan Velinov",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrryai/pepper.git",
    "directory": "packages/pepper"
  },
  "bugs": {
    "url": "https://github.com/chrryai/pepper/issues"
  },
  "homepage": "https://github.com/chrryai/pepper",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@types/react": "^19.1.12"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
