    "markdown-to-jsx": "^7.7.13",
    "moti": "^0.30.0",
    "motion": "^12.23.24",
    "next-auth": "^4.24.11",
    "nextjs-toploader": "^3.8.16",
    "nprogress": "^0.2.0",
    "react": "^19.1.1",
    "react-audio-play": "^1.0.4",
    "react-circle-flags": "^0.0.25",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^16.5.4",
