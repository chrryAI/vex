    "hermes-parser": "^0.18.2",
    "meriyah": "^4.3.9",
    "oxc-parser": "^0.72.2"
  },
  "bin": {
    "porf": "./runtime/index.js"
  },
  "main": "./compiler/wrap.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CanadaHonk/porffor.git"
