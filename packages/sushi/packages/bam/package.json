{
  "name": "@chrryai/bam",
  "version": "0.1.0",
  "description": "Bug Analysis & Memory - Detect bugs, logic errors, and missing implementations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "keywords": [
    "bug-detection",
    "static-analysis",
    "code-quality",
    "falkordb",
    "graph-database"
  ],
  "author": "Chrry AI",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "falkordb": "^6.6.0"
  }
}
