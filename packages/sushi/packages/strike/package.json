{
  "name": "@chrryai/strike",
  "version": "0.1.0",
  "description": "Mutation Testing System - Generate mutants, run tests, identify weak spots",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "keywords": ["mutation-testing", "test-quality", "code-coverage", "falkordb"],
  "author": "Chrry AI",
  "license": "MIT",
  "dependencies": {
    "falkordb": "^1.0.5"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
